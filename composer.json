{
  "name": "nlksoft/laravel-responsive-filemanager",
  "description": "Responsive File Manager with Laravel",
  "keywords": [
    "laravel",
    "tinymce",
    "ckeditor",
    "responsive",
    "filemanager",
    "laravel-filemanager",
    "responsive-filemanager",
    "tinymce-filemanager"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Abdulkadir KAR",
      "email": "kadir@nlksoft.com"
    }
  ],
  "require": {
    "php": "^7.3 | ^8.0 | ^8.1",
    "illuminate/support": "^7.0 | ^8.0 | ^9.0"
  },
  "autoload": {
    "psr-4": {
      "Snowsoft\\ResponsiveFileManager\\": "src/",
      "Snowsoft\\FtpClient\\": "src/FtpClient/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Snowsoft\\ResponsiveFileManager\\FileManagerServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev"
}
